<Window x:Class="FFmpeg_Wrapper_WPF.NET.OptionsMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FFmpeg_Wrapper_WPF.NET"
        mc:Ignorable="d"
        Title="Options" Height="300" Width="200">
    <DockPanel>
        <ToolBarTray DockPanel.Dock="Top">
            <ToolBar>
                <Button Content="About"/>
                <Button Content="Processing"/>
            </ToolBar>
        </ToolBarTray>
        <Frame Grid.Row="1" x:Name="frame" NavigationUIVisibility="Hidden"/>
    </DockPanel>
</Window>
